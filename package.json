{
  "name": "tmk-web",
  "scripts": {
    "version": "0.0.0",
    "license": "MIT",
    "ng": "ng",
    "start": "npm run start:debug",
    "start:debug": "ng serve --open --environment=debug --deploy-url=/",
    "start:prod": "ng serve --open --environment=prod --target=production --aot --deploy-url=/",
    "start:dev": "ng serve --open --environment=dev --deploy-url=/",
    "start:prod-old": "ng serve --open --environment=prod-old --deploy-url=/",
    "start:debug-hmr": "ng serve --open --environment=debug --hmr --deploy-url=/",
    "start:prod-hmr": "ng serve --open --environment=prod --hmr --deploy-url=/",
    "start:dev-hmr": "ng serve --open --environment=dev --hmr --deploy-url=/",
    "stop": "sudo kill `sudo lsof -t -i:4600`",
    "build:prod": "ng build --aot --environment=prod --target=production",
    "build:dev": "ng build --aot --environment=dev --target=production",
    "build:prod-old": "ng build --aot --environment=prod-old --target=production",
    "build": "npm run build:prod",
    "ngc": "ngc",
    "test": "ng test",
    "test-coverage": "ng test --code-coverage=true",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "versions": " ng -v && tsc -v",
    "genicons": "webfont ./src/assets/icons/web-*.svg -f WebIcons -d ./src/assets/icons -s ./src -t scss -c icon -p /assets/icons/",
    "publish": "git checkout deploy && git pull && git merge master && git push && git checkout master",
    "compodoc": "./node_modules/.bin/compodoc -p src/tsconfig.app.json"
  },
  "webfont": {
    "normalize": true
  },
  "private": true,
  "dependencies": {
    "@angular-redux/form": "6.6.0",
    "@angular-redux/router": "6.3.1",
    "@angular-redux/store": "6.6.0",
    "@angular/animations": "5.2.9",
    "@angular/cdk": "5.2.0",
    "@angular/common": "5.2.9",
    "@angular/compiler": "5.2.9",
    "@angular/core": "5.2.9",
    "@angular/forms": "5.2.9",
    "@angular/http": "5.2.9",
    "@angular/material": "5.2.0",
    "@angular/platform-browser": "5.2.9",
    "@angular/platform-browser-dynamic": "5.2.9",
    "@angular/router": "5.2.9",
    "@types/moment": "2.13.0",
    "accounting": "0.4.1",
    "angular2-text-mask": "8.0.4",
    "core-js": "2.5.1",
    "d3": "4.10.0",
    "file-saver": "1.3.3",
    "jquery": "3.2.1",
    "lodash": "4.17.4",
    "moment": "2.19.1",
    "ng-simple-storage": "1.0.1",
    "ng2-dragula": "1.5.0",
    "ngx-malihu-scrollbar": "1.3.1",
    "ngx-toastr": "8.2.1",
    "redux": "3.7.2",
    "redux-logger": "3.0.6",
    "rxjs": "5.5.8",
    "select2-component": "3.1.7",
    "signalr": "2.2.2",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/core": "0.5.6",
    "@angular/cli": "1.7.4",
    "@angular/compiler-cli": "5.2.9",
    "@angular/language-service": "5.2.9",
    "@angularclass/hmr": "2.1.3",
    "@compodoc/compodoc": "1.0.6",
    "@types/accounting": "0.4.1",
    "@types/d3": "4.12.0",
    "@types/file-saver": "1.3.0",
    "@types/geojson": "7946.0.2",
    "@types/jasmine": "2.6.0",
    "@types/jquery": "3.2.8",
    "@types/lodash": "4.14.104",
    "@types/mcustomscrollbar": "2.8.32",
    "@types/node": "8.0.14",
    "@types/redux-logger": "3.0.5",
    "@types/signalr": "2.2.34",
    "codelyzer": "4.1.0",
    "jasmine-core": "2.99.1",
    "karma": "2.0.0",
    "karma-chrome-launcher": "2.1.1",
    "karma-coverage-istanbul-reporter": "1.3.0",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "redux-devtools-extension": "2.13.2",
    "ts-node": "3.3.0",
    "tslint": "5.9.1",
    "typescript": "2.6.2"
  }
}
